package nationwide.calculator;
import java.awt.event.*;
import java.awt.*;


public class division implements ActionListener {
	TextField no1, no2, result;
public division (TextField A, TextField B, TextField C) {
	no1=A;
	no2=B;
	result=C;
}
public void actionPerformed (ActionEvent X) {
	int A, B;
	double C;
	A=Integer.parseInt(no1.getText());
	B=Integer.parseInt(no2.getText());
	C=A/B;
	result.setText(Double.toString(C));
			}

}


